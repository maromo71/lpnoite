#include <stdio.h>

int main(void) {
  int razao;
  printf("Digite um valor para a razao da serie: \n");
  scanf("%d", &razao);
  int t=1; //primeiro termo dado pelo enunciado
  for (int i=1; i<=10; i++){
    printf("%d\n", t); //impressao dos termos
    t+=razao; //soma dos termos
  }
  return 0;
}